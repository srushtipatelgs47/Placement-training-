#include <stdio.h>

int areEqual(int a, int b) {
    return !(a - b);
}

int main() {
    int num1, num2;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    if (areEqual(num1, num2)) {
        printf("%d and %d are equal.\n", num1, num2);
    } else {
        printf("%d and %d are not equal.\n", num1, num2);
    }

    return 0;
}
